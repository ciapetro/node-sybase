{
    "name": "ciapetro-sybase",
    "version": "1.2.2",
    "description": "A simple node.js wrapper around a java/jconnect app that provides easy access to Sybase Databases without having to install odbc or freetds.",
    "dependencies": {
        "JSONStream": "latest"
    },
    "main": "src/SybaseDB.js",
    "scripts": {
        "test": "mocha -r ts-node/register test/**/*.ts",
        "prebuild": "rimraf dist",
        "copyJava": "mkdir  \"dist/JavaSybaseLink/dist/lib\" &&  @powershell copy './JavaSybaseLink/dist/*.*' './dist/JavaSybaseLink/dist' &&  @powershell copy './JavaSybaseLink/dist/lib/*.*' './dist/JavaSybaseLink/dist/lib'",
        "copyPackage": "@powershell copy 'package.json' './dist'",
        "build": "tsc",
        "postbuild": "npm run copyJava && npm run copyPackage && cd dist && npm install --only=production"
    },
    "homepage": "https://github.com/ciapetro/node-sybase",
    "repository": {
        "type": "git",
        "url": "git://github.com/ciapetro/node-sybase.git"
    },
    "license": "MIT",
    "keywords": [
        "database",
        "sybase",
        "jconnect",
        "sql",
        "java"
    ],
    "author": "Ciapetro <ciapetro@desenvolvimento.vom.br>",
    "devDependencies": {
        "@types/bluebird": "^3.5.32",
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.0.9",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "bluebird": "^3.4.6",
        "chai": "^3.5.0",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "mocha": "^3.0.2",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "^3.9.2",
        "dotenv": "^8.2.0"
    }
}
